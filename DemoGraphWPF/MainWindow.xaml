<Window x:Class="DemoGraphWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="825"  >
    <StackPanel Orientation="Horizontal">
        <Image Width="200" Source="{Binding Name1}"/>
        <Image Width="200" Source="{Binding Name2}"/>
        <Image Width="200"/>
        <StackPanel>
            <Button Content="Генерировать изоморфный граф"/>
            <Button Content="Доказать изоморфизм графу №1"/>
            <Button Content="Доказать изоморфизм графу №2"/>
        </StackPanel>
    </StackPanel>


</Window>
